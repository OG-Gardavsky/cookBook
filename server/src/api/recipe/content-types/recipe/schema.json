{
  "kind": "collectionType",
  "collectionName": "recipes",
  "info": {
    "singularName": "recipe",
    "pluralName": "recipes",
    "displayName": "Recipe",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "photo": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "title": {
      "type": "string"
    },
    "timeToPrepare": {
      "type": "integer",
      "min": 0
    },
    "description": {
      "type": "text"
    },
    "tools": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::tool.tool",
      "inversedBy": "recipes"
    },
    "difficulty": {
      "type": "enumeration",
      "enum": [
        " 1",
        " 2",
        " 3",
        " 4",
        " 5"
      ]
    }
  }
}
