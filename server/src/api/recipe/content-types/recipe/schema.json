{
  "kind": "collectionType",
  "collectionName": "recipes",
  "info": {
    "singularName": "recipe",
    "pluralName": "recipes",
    "displayName": "Recipe",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "photo": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images"
      ],
      "pluginOptions": {}
    },
    "title": {
      "type": "string",
      "pluginOptions": {}
    },
    "timeToPrepare": {
      "type": "integer",
      "min": 0,
      "pluginOptions": {}
    },
    "directions": {
      "type": "text",
      "pluginOptions": {}
    },
    "tools": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::tool.tool",
      "inversedBy": "recipes"
    },
    "difficulty": {
      "type": "enumeration",
      "enum": [
        " 1",
        " 2",
        " 3",
        " 4",
        " 5"
      ],
      "pluginOptions": {}
    },
    "ingredients": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::ingredient.ingredient",
      "inversedBy": "recipes"
    },
    "likes": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::like.like",
      "mappedBy": "recipe"
    },
    "category": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::category.category",
      "inversedBy": "recipes"
    }
  }
}
